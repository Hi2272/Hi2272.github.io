public class Farbe{
   private int rot, gruen, blau;
 
  public Farbe(int rot, int gruen, int blau){
    this.rot=rot;
    this.gruen=gruen;
    this.blau=blau;
  
}

public int getRot() {
    return this.rot;
  }
  public int getBlau() {
    return this.blau;
  }
  public int getGruen() {
    return this.gruen;
  }
  public void setRot(int rot){
    this.rot=rot;
  }
  public void setBlau(int blau){
    this.blau=blau;
  }
  public void setGruen(int gruen){
    this.gruen=gruen;
  }
  

}
